<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block title %}StreetHunt{% endblock %}</title>
        <link rel="stylesheet" href="{{ asset('assets/f/css/main.css') }}">
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}?v=1" />
    </head>

    <body id="page-top" class="index" cz-shortcut-listen="true">
        <div class="progress-bar-loading">
            <div class="progress-bar-loading--elem">

            </div>
        </div>

        <div id="main">
            {% include 'front/default/_menu.html.twig' %}

            <div class="container">
                <div class="row">
                    {% block leftside %}{% endblock %}
                    {% block body %}{% endblock %}
                    {% block rightside %}{% endblock %}
                </div>
            </div>

            {% include 'front/default/_footer.html.twig' %}
        </div>

        {% block javascripts %}{% endblock %}
        <script src="{{ asset('assets/f/js/main.js') }}"></script>
    </body>
</html>
